{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "class-methods-use-this":"off",
        "camelcase":"off",
        "import/prefer-default-export":"off",
        "consistent-return": "off"
    }
}